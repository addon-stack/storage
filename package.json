{
  "name": "@addon-core/storage",
  "version": "0.2.0",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/addon-stack/storage"
  },
  "homepage": "https://github.com/addon-stack/storage#readme",
  "bugs": {
    "url": "https://github.com/addon-stack/storage/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "rslib build",
    "dev": "rslib build --watch",
    "format": "biome format --write .",
    "format:check": "biome check --formatter-enabled=true --linter-enabled=false .",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:fix:unsafe": "biome check --write --unsafe .",
    "test": "jest",
    "test:ci": "jest --ci --passWithNoTests",
    "test:related": "jest --bail --passWithNoTests --findRelatedTests",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "release": "changeset publish"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "biome check --write --unsafe",
      "npm run test:related --"
    ],
    "*.{json,md,mdx,css,scss,html}": [
      "biome format --write"
    ]
  },
  "overrides": {
    "glob": "^10.3.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@rslib/core": "^0.13.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/chrome": "^0.1.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.4",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-webextension-mock": "^4.0.0",
    "lint-staged": "^15.2.10",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "@microsoft/api-extractor": "^7.47.9"
  },
  "dependencies": {
    "@adnbn/browser": "^0.1.7"
  },
  "peerDependencies": {
    "@types/chrome": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  }
}
